<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <PackageId>Sia.Inspector.API</PackageId>
    <PackageDescription>WebAPI for Sia.WebInspector</PackageDescription>
    <Author>Phlamcenth Sicusa</Author>
    <Company>Gilatod.art</Company>
    <RepositoryUrl>https://github.com/sicusa/Sia.WebInspector</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <RootNamespace>Sia.WebInspector.API</RootNamespace>
  </PropertyGroup> 

  <ItemGroup>
    <PackageReference Include="Sia" Version="1.9.1" />
    <PackageReference Include="Sia.Reactors" Version="1.9.1" />
    <PackageReference Include="Sia.CodeGenerators" Version="1.9.1" />
    <PackageReference Include="CommunityToolkit.HighPerformance" Version="8.2.1" />
    <PackageReference Include="System.Reactive" Version="6.0.1-preview.1" />
  </ItemGroup>

</Project>
